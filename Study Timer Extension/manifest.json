{
  "manifest_version" : 3,
  "name" : "My Extension",
  "version": "1.0.0",
  "description" : "Hello Extension!",
  "icons" : {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
  },
  "action":{
      "default_icon" : {
          "16": "icon.png",
          "48": "icon.png",
          "32": "icon.png"
      },
      "default_title": "Default Extension Timer",
      "default_popup": "popup.html"
  },
  "options_page": "options.html",
  "permissions" : ["storage","alarms","notifications"],
  "background":{ 
    "service_worker" : "background.js"
    }
}
